@{
	Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">
<head>
	<title>@ViewData["Title"] - Lebron</title>
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta charset="utf-8">
	<meta name="keywords" content="Weblog a Responsive web template, Bootstrap Web Templates, Flat Web Templates, Android Compatible web template,
Smartphone Compatible web template, free webdesigns for Nokia, Samsung, LG, SonyEricsson, Motorola web design" />

	<script>
		addEventListener("load", function () {
			setTimeout(hideURLbar, 0);
		}, false);

		function hideURLbar() {
			window.scrollTo(0, 1);
		}
	</script>
	<link href="/CoreBlogTema/web/css/bootstrap.css" rel='stylesheet' type='text/css' />
	<link rel="stylesheet" href="/CoreBlogTema/web/css/single.css">
	<link href="/CoreBlogTema/web/css/style.css" rel='stylesheet' type='text/css' />
	<link href="/CoreBlogTema/web/css/fontawesome-all.css" rel="stylesheet">
	<link href="//fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800"
		  rel="stylesheet">
</head>
<body>
	<!--Header-->
	<header>
		<div class="top-bar_sub_w3layouts container-fluid">
			<div class="row">
				<div class="col-md-4 logo text-left">
					<a class="navbar-brand" href="/CoreBlogTema/web/index.html">
						<img src="/writer/assets/images/new-logo/logo-no-background.svg" alt="logo" style="max-width: 120px" />
						@* <i class="fab fa-linode"></i> LEBRON *@
					</a>
				</div>

				@await Html.PartialAsync("Partial/Hosgeldiniz")

				@await Html.PartialAsync("Partial/SocialMedia")
			</div>
		</div>
		@await Html.PartialAsync("Partial/HeaderButtons")
	</header>
	<!--//header-->
	<!--/banner-->
	<ol class="breadcrumb">
		<li class="breadcrumb-item">
			<a href="/CoreBlogTema/web/index.html">Home</a>
		</li>
		<li class="breadcrumb-item active">Blog3</li>
	</ol>
	<!--//banner-->
	@RenderBody()

	<!--footer-->
	@await Html.PartialAsync("Partial/Footer")

	<!---->
	<!-- js -->
	<script src="/CoreBlogTema/web/js/jquery-2.2.3.min.js"></script>
	<!-- //js -->
	<!--/ start-smoth-scrolling -->
	<script src="/CoreBlogTema/web/js/move-top.js"></script>
	<script src="/CoreBlogTema/web/js/easing.js"></script>
	<script>
		jQuery(document).ready(function ($) {
			$(".scroll").click(function (event) {
				event.preventDefault();
				$('html,body').animate({
					scrollTop: $(this.hash).offset().top
				}, 900);
			});
		});
	</script>
	<!--// end-smoth-scrolling -->

	<script>
		$(document).ready(function () {
			/*
									var defaults = {
											containerID: 'toTop', // fading element id
										containerHoverID: 'toTopHover', // fading element hover id
										scrollSpeed: 1200,
										easingType: 'linear'
										};
									*/

			$().UItoTop({
				easingType: 'easeOutQuart'
			});

		});
	</script>

	<a href="#home" class="scroll" id="toTop" style="display: block;">
		<span id="toTopHover" style="opacity: 1;"> </span>
	</a>

	<!-- //Custom-JavaScript-File-Links -->
	<script src="/CoreBlogTema/web/js/bootstrap.js"></script>
</body>
</html>
